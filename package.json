{
  "name": "cpu-benchmark-backend",
  "version": "1.0.0",
  "description": "Backend for CPU Benchmark application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "build": "npm install",
    "migrate": "node ./node_modules/.bin/sequelize-cli db:migrate",
    "migrate:undo": "node ./node_modules/.bin/sequelize-cli db:migrate:undo",
    "migrate:undo:all": "node ./node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "seed": "node ./node_modules/.bin/sequelize-cli db:seed:all",
    "seed:undo": "node ./node_modules/.bin/sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.7",
    "speakeasy": "^2.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
